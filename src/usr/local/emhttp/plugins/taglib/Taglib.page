<script>
    fetch('http://localhost:5173/index.html')
        .then(response => response.text()) // 将响应转化为文本
        .then(html => {
            // 创建一个DOMParser实例
            const parser = new DOMParser();
            // 解析HTML字符串为一个文档对象
            const doc = parser.parseFromString(html, 'text/html');
            // 你可以直接操作这个文档对象
            const content = doc.getElementById('app'); // 假设你要获取example.html中的一个特定元素
            document.getElementById('addDiv').appendChild(content); // 将获取的内容插入到当前文档的指定位置

            import('http://localhost:5173/src/main.js')
              .then(module => {
                console.log(module);
              })
              .catch(error => {
                console.error(error);
              });
        })
        .catch(error => {
            console.error('Error fetching HTML:', error);
        });
</script>

<div id="addDiv"></div>
